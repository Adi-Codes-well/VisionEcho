{
  "name": "visionecho",
  "version": "1.0.0",
  "description": "VisionEcho - AI-powered computer vision application",
  "scripts": {
    "dev": "concurrently \"npm run server\" \"npm run client\" \"npm run ai\"",
    "server": "cd server && npm run dev",
    "client": "cd client && npm run dev",
    "ai": "cd ai-service && python app.py",
    "build": "cd client && npm run build",
    "start": "cd server && npm start",
    "install-all": "npm install && cd client && npm install && cd ../server && npm install && cd ..",
    "test": "cd client && npm test",
    "lint": "cd client && npm run lint"
  },
  "keywords": ["react", "nodejs", "express", "python", "ai", "computer-vision"],
  "author": "Aditya Jha",
  "license": "MIT",
  "devDependencies": {
    "concurrently": "^8.2.2"
  }
}